				<safety>
					<safecond>
						<warning>
							<para>
								Attenersi alle precauzioni di sicurezza generali riportate al para <refdm>
									<avee>
										<modelic>THSAAAAAAAAAAA</modelic>
										<sdc>AAAA</sdc>
										<chapnum>HC6</chapnum>
										<section>3</section>
										<subsect>0</subsect>
										<subject>0000</subject>
										<discode>01</discode>
										<discodev>AAA</discodev>
										<incode>012</incode>
										<incodev>A</incodev>
